public class Main {
    public static void main(String[] args) {
        int number = 100;
        printPrimeBefore(number);
    }

    public static boolean isPrime(int num) {
        if (num <= 1)
            return false;
        for (int i = 2; i < num; i++)
            if (num % i == 0)
                return false;
        return true;
    }

    public static void printPrimeBefore(int number) {
        for (int i = 2; i <= number; i++) {
            if (isPrime(i)) {
                System.out.print(i + " ");
            }
        }
    }
}